<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.goblincwl.cwlweb.index.mapper.BadWordsMapper">

	<select id="selectList" resultType="com.goblincwl.cwlweb.index.entity.BadWords">
		SELECT BW.ID AS "id",
			BW.WORD AS "word",
			BW.TYPE AS "type"
		FROM BAD_WORDS BW
	</select>
</mapper>